  <appSettings>
    <add key="CONCURRENT_FACTORY_INSTANCE_COUNT" value="2" />
    <add key="MAX_INPUTTEXT_LENGTH"              value="1000000" />

    <!--<add key="NOT_USE_RESOURCE_ASSEMBLY" value="true" />-->
